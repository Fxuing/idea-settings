<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Debug 'App'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'App'</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug with JRebel 'App'">
        <Item>
          <IdeaActionID>JRebel Debug</IdeaActionID>
          <ShortCut>'Ctrl+Alt+0'</ShortCut>
          <Description>Debug with JRebel 'App'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
      <Statistic Action="MyBatis Log Plugin">
        <Item>
          <IdeaActionID>TailMyBatisLog</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Shift+O'</ShortCut>
          <Description>MyBatis Log Plugin</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Quick Evaluate Expression">
        <Item>
          <IdeaActionID>QuickEvaluateExpression</IdeaActionID>
          <ShortCut>'Ctrl+Alt+F8' or 'Alt+Button1 Click'</ShortCut>
          <Description>Quick Evaluate Expression</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'App'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'App'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'App'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'App'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'Alt+7'</ShortCut>
          <Description>Structure</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>